    "js-beautify": "^1.7.5",
    "merge-stream": "^1.0.0",
    "mocha": ">=2.2.5",
    "nyc": "^11.6.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.0.2",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "vulcanize": ">= 1.4.2",
    "web-component-tester": "^4.0.0"
  },
  "bin": {
    "webda": "./bin/webda"
  },
  "files": [
    "app",
    "bin/webda",
    "lib",
    "logo.json"
