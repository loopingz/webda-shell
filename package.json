  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "test": "nyc mocha --timeout=100000",
    "run": "node core",
    "format": "node ./scripts/format.js",
    "start": "bin/webda",
    "precommit": "npm run format && npm run test"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "dependencies": {
    "archiver": "^1.0.0",
    "aws-sdk": "^2.223.1",
    "body-parser": ">=1.13.3",
    "colors": "^1.2.1",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": ">=1.11.3",
    "filesize": "^3.6.1",
    "fs-finder": "^1.8.1",
    "glob": "^7.1.2",
    "merge": "^1.2.0",
    "mime-types": ">=2.1.4",
    "mkdirp": "^0.5.1",
    "multer": ">=1.1.0",
    "nodejs-websocket": "^1.5.0",
    "open": "0.0.5",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "socket.io": "^2.0.3",
    "webda": "~0.9.12",
    "yargs": "^7.1.0",
    "yauzl": "^2.7.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".js"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "report-dir": "./reports",
    "all": true
